#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include "basic-types.h"
#include "memory.h"
#include "bitArray.h"


int
main(int argc, char**argv) {

  bitarray a;

  a = initbitarray(NULL, 10);
  dumpbitarray(a, 10);
  setbitarray(a, 10, 0);
  dumpbitarray(a, 10);
  setbit(a, 5, 1);
  dumpbitarray(a, 10);
  return 0;
}



